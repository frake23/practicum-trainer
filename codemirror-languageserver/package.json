    "@codemirror/state": "^6.1.2",
    "@codemirror/view": "^6.3.0",
    "@open-rpc/client-js": "^1.7.1",
    "eslint": "^8.24.0",
    "global": "^4.4.0",
    "vscode-languageserver-protocol": "^3.17.2"
  }
}
